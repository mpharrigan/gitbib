{% for entries in [entries1, entries2] -%}
{% for entry in entries -%}
@{{ entry.type | type }}{{ '{'+entry.ident }},
  author    = "{{ entry.authors | authors }}",
  title     = "{{ entry.title | title }}",
  {% if entry.booktitle -%}
  booktitle = "{{ entry.booktitle }}",
  {% endif -%}
  {% if entry.published_online -%}
  year      = "{{ entry.published_online | published_online }}",
  {% elif entry.published_print -%}
  year      = "{{entry.published_print | published_print }}",
  {% endif -%}
  {% if entry.container_title -%}
  journal   = "{{entry.container_title | container_title }}",
  {% endif -%}
  {% if entry.address -%} {# note: I don't think this is ever given by crossref #}
  address   = "{{entry.address}}",
  {% endif -%}
  {% if entry.volume -%}
  volume    = "{{entry.volume}}",
  {% endif -%}
  {% if entry.issue -%}
  number    = "{{entry.issue}}",
  {% endif -%}
  {% if entry.chapter -%}
  chapter   = "{{entry.chapter}}",
  {% endif -%}
  {% if entry.page -%}
  pages     = "{{ entry.page | page }}",
  {% endif -%}
  {% if entry.arxiv -%}
  archivePrefix = "arXiv",
  eprint        = "{{ entry.arxiv }}",
  primaryClass  = "{{ entry.arxiv_category }}",
  {% elif entry.biorxiv -%}
  note      = "bioRxiv:{{ entry.biorxiv }}",
  {% endif -%}
  {% if entry.publisher -%}
  publisher = "{{entry.publisher}}",
  {% endif -%}
  {% if not entry.doi and entry.biorxiv -%}
  doi       = "{{ entry.biorxiv }}",
  {% elif entry.doi -%}
  doi       = "{{ entry.doi }}",
  {% endif %}
}
{% endfor %}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%


{% endfor %}
